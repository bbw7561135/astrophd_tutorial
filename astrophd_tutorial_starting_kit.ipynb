    "import numpy as np\n",
    "\n",
    "class ObjectDetector:\n",
    "    def __init__(self):\n",
    "        pass\n",
    "\n",
    "    def fit(self, X, y):\n",
    "        return self\n",
    "\n",
    "    def predict(self, X):\n",
    "        return np.zeros_like(X)"
   ]
